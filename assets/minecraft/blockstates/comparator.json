{
  "multipart": [
  
	{	"when": { "facing": "south" },
			"apply": { "model": "block/comparator/base"}
	},

	{	"when": { "facing": "west" },
			"apply": { "model": "block/comparator/base", "y": 90}
	},
		
	{	"when": { "facing": "north" },
			"apply": { "model": "block/comparator/base", "y": 180}
	},
		
	{	"when": { "facing": "east" },
			"apply": { "model": "block/comparator/base", "y": 270}
	},
	
	
	{	"when": { "facing": "south", "mode": "compare"  },
			"apply": { "model": "block/comparator/subtract_off"}
	},

	{	"when": { "facing": "west", "mode": "compare"  },
			"apply": { "model": "block/comparator/subtract_off", "y": 90}
	},
		
	{	"when": { "facing": "north", "mode": "compare"  },
			"apply": { "model": "block/comparator/subtract_off", "y": 180}
	},
		
	{	"when": { "facing": "east", "mode": "compare"  },
			"apply": { "model": "block/comparator/subtract_off", "y": 270}
	},
	
	
	{	"when": { "facing": "south", "mode": "subtract"  },
			"apply": { "model": "block/comparator/subtract_on"}
	},

	{	"when": { "facing": "west", "mode": "subtract"  },
			"apply": { "model": "block/comparator/subtract_on", "y": 90}
	},
		
	{	"when": { "facing": "north", "mode": "subtract"  },
			"apply": { "model": "block/comparator/subtract_on", "y": 180}
	},
		
	{	"when": { "facing": "east", "mode": "subtract"  },
			"apply": { "model": "block/comparator/subtract_on", "y": 270}
	},
	
	
	{	"when": { "facing": "south", "powered": "false"  },
			"apply": { "model": "block/comparator/powered_off"}
	},

	{	"when": { "facing": "west", "powered": "false"  },
			"apply": { "model": "block/comparator/powered_off", "y": 90}
	},
		
	{	"when": { "facing": "north", "powered": "false"  },
			"apply": { "model": "block/comparator/powered_off", "y": 180}
	},
		
	{	"when": { "facing": "east", "powered": "false"  },
			"apply": { "model": "block/comparator/powered_off", "y": 270}
	},
	
	
	{	"when": { "facing": "south", "powered": "true"  },
			"apply": { "model": "block/comparator/powered_on"}
	},

	{	"when": { "facing": "west", "powered": "true"  },
			"apply": { "model": "block/comparator/powered_on", "y": 90}
	},
		
	{	"when": { "facing": "north", "powered": "true"  },
			"apply": { "model": "block/comparator/powered_on", "y": 180}
	},
		
	{	"when": { "facing": "east", "powered": "true"  },
			"apply": { "model": "block/comparator/powered_on", "y": 270}
	}
	
  ]
}