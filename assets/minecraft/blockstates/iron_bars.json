{
    "multipart": [
        {   "apply": { "model": "block/iron_bars_post" }},

        {   "when": { "north": true },
            "apply": { "model": "block/iron_bars_side_0" }
        },
        {   "when": { "east": true },
            "apply": { "model": "block/iron_bars_side_0", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "block/iron_bars_side_1"}
        },
        {   "when": { "west": true },
            "apply": { "model": "block/iron_bars_side_1", "y": 90 }
        }
    ]
}
