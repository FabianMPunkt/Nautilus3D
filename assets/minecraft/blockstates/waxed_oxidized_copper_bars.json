{
    "multipart": [
        {   "apply": { "model": "block/oxidized_copper_bars_post" }},

        {   "when": { "north": true },
            "apply": { "model": "block/oxidized_copper_bars_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "block/oxidized_copper_bars_side", "y": 90 }
        },
        {   "when": { "south": true },
            "apply": { "model": "block/oxidized_copper_bars_side_alt"}
        },
        {   "when": { "west": true },
            "apply": { "model": "block/oxidized_copper_bars_side_alt", "y": 90 }
        }
    ]
}
